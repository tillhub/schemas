{
  "$id": "https://schemas.tillhub.com/v0/voucher_systems.get.query.schema.json",
  "$schema": "http://json-schema.org/draft-07/schema#",
  "additionalProperties": true,
  "type": "object",
  "required": [],
  "properties": {
    "start": {
      "type": "string",
      "format": "date-time"
    },
    "end": {
      "type": "string",
      "format": "date-time"
    },
    "deleted": {
      "type": "string",
      "enum": [
        "true",
        "false"
      ]
    },
    "active": {
      "type": "string",
      "enum": [
        "true",
        "false"
      ]
    },
    "q": {
      "description": "Search by voucher system name, with a partial match",
      "example": "abocard",
      "type": "string"
    }
  }
}
