{
  "$id": "https://schemas.tillhub.com/v0/analytics.discounts.request.schema.json",
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "additionalProperties": false,
  "properties": {
    "limit": {
      "type": "string",
      "maxLength": 4
    },
    "format": {
      "type": "string",
      "enum": [
        "csv"
      ]
    },
    "start": {
      "oneOf": [
        {
          "type": "string",
          "format": "date"
        },
        {
          "type": "string",
          "format": "date-time"
        }
      ]
    },
    "end": {
      "oneOf": [
        {
          "type": "string",
          "format": "date"
        },
        {
          "type": "string",
          "format": "date-time"
        }
      ]
    },
    "discount": {
      "type": "string"
    },
    "product": {
      "type": "string"
    },
    "branch": {
      "type": "string"
    },
    "discount_rate": {
      "type": "string"
    },
    "export_map": {
      "type": "object",
      "properties": {
        "fields": {
          "type": "object"
        }
      }
    }
  },
  "required": []
}
