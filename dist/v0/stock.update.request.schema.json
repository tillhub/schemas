{
  "$id": "https://schemas.tillhub.com/v0/stock.update.request.schema.json",
  "$schema": "http://json-schema.org/draft-07/schema#",
  "additionalProperties": false,
  "type": "object",
  "required": [
    "qty"
  ],
  "properties": {
    "qty": {
      "type": "number"
    },
    "metadata": {
      "oneOf": [
        {
          "type": "object"
        },
        {
          "type": "null"
        }
      ]
    },
    "reason": {
      "oneOf": [
        {
          "type": "string",
          "format": "uuid"
        },
        {
          "type": "null"
        }
      ]
    }
  }
}
