                "example": "EUR",
                "minLength": 3,
                "maxLength": 3
              },
              "units": {
                "type": "array",
                "additionalProperties": false,
                "description": "units.",
                "items": {
                  "type": "object",
                  "description": "Cash unit details pre e.g. 7 pieces of 10 Cents coins.",
                  "properties": {
                    "type": {
                      "type": "string",
                      "example": "coin",
                      "enum": [
                        "bill",
