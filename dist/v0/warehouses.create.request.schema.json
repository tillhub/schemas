{
  "$id": "https://schemas.tillhub.com/v0/warehouses.create.request.schema.json",
  "$schema": "http://json-schema.org/draft-07/schema#",
  "properties": {
    "name": {
      "anyOf": [
        {
          "type": "string",
          "maxLength": 64,
          "minLength": 1
        },
        {
          "type": "null"
        }
      ]
    },
    "phonenumbers": {
      "anyOf": [
        {
          "type": "object",
          "additionalProperties": false,
          "properties": {
            "line_main": {
              "type": "string"
            },
            "line_1": {
              "type": "string"
            },
            "line_2": {
              "type": "string"
            }
          },
          "anyOf": [
            {
              "required": [
                "line_main"
              ]
            },
            {
              "required": [
                "line_1"
              ]
            },
            {
              "required": [
                "line_2"
              ]
            }
          ]
        },
        {
          "type": "null"
        }
      ]
    },
    "images": {
      "anyOf": [
        {
          "type": "object",
          "additionalProperties": false,
          "properties": {
            "original": {
              "type": "string",
              "format": "uri"
            },
            "gallery": {
              "type": "string",
              "format": "uri"
            },
            "avatar": {
              "type": "string",
              "format": "uri"
            },
            "1x": {
              "type": "string",
              "format": "uri"
            },
            "2x": {
              "type": "string",
              "format": "uri"
            },
            "3x": {
              "type": "string",
              "format": "uri"
            }
          }
        },
        {
          "type": "null"
        }
      ]
    },
    "contacts": {
      "anyOf": [
        {
          "type": "object",
          "additionalProperties": false,
          "properties": {
            "email": {
              "type": "string",
              "format": "email"
            }
          }
        },
        {
          "type": "null"
        }
      ]
    },
    "active": {
      "type": "boolean",
      "default": true
    },
    "deleted": {
      "type": "boolean",
      "default": false
    },
    "metadata": {
      "type": "object"
    },
    "location": {
      "type": "object"
    },
    "configuration": {
      "anyOf": [
        {
          "type": "string",
          "format": "uuid"
        },
        {
          "type": "null"
        }
      ]
    },
    "custom_id": {
      "type": "string",
      "minLength": 1,
      "maxLength": 64
    },
    "capacity": {
      "anyOf": [
        {
          "type": "number"
        },
        {
          "type": "null"
        }
      ]
    },
    "short_name": {
      "anyOf": [
        {
          "type": "string"
        },
        {
          "type": "null"
        }
      ]
    },
    "barcode": {
      "anyOf": [
        {
          "type": "string"
        },
        {
          "type": "null"
        }
      ]
    },
    "addresses": {
      "oneOf": [
        {
          "description": "A Tillhub address array, with different types of addresses. More description in the properties...",
          "type": "array",
          "maxItems": 3,
          "items": {
            "type": "object",
            "additionalProperties": false,
            "description": "Downstream delivery of a date.",
            "required": [
              "lines",
              "street",
              "locality",
              "region",
              "postal_code",
              "country"
            ],
            "properties": {
              "lines": {
                "oneOf": [
                  {
                    "description": "Address details",
                    "example": "Penthouse",
                    "type": "array",
                    "minItems": 1,
                    "maxItems": 4,
                    "items": {
                      "type": "string",
                      "minLength": 1
                    }
                  },
                  {
                    "type": "null"
                  }
                ]
              },
              "street": {
                "oneOf": [
                  {
                    "description": "Street name",
                    "example": "Genthiner Str.",
                    "type": "string"
                  },
                  {
                    "type": "null"
                  }
                ]
              },
              "street_number": {
                "oneOf": [
                  {
                    "description": "Building number",
                    "example": "34",
                    "type": "string"
                  },
                  {
                    "type": "null"
                  }
                ]
              },
              "locality": {
                "oneOf": [
                  {
                    "description": "Locality name (city, village, etc.)",
                    "example": "Berlin",
                    "type": "string"
                  },
                  {
                    "type": "null"
                  }
                ]
              },
              "region": {
                "oneOf": [
                  {
                    "description": "Region name",
                    "example": "Branderburg",
                    "type": "string"
                  },
                  {
                    "type": "null"
                  }
                ]
              },
              "postal_code": {
                "oneOf": [
                  {
                    "description": "Postal code",
                    "example": "10785",
                    "type": "string",
                    "maxLength": 10
                  },
                  {
                    "type": "null"
                  }
                ]
              },
              "country": {
                "oneOf": [
                  {
                    "description": "A country as ISO Alpha-2 code.",
                    "example": "DE",
                    "type": "string",
                    "minLength": 2,
                    "maxLength": 2,
                    "pattern": "^[A-Z]{2}$"
                  },
                  {
                    "type": "null"
                  }
                ]
              },
              "type": {
                "oneOf": [
                  {
                    "description": "Address type",
                    "example": "delivery",
                    "type": "string",
                    "enum": [
                      "local",
                      "delivery",
                      "billing",
                      "home",
                      "work"
                    ]
                  },
                  {
                    "type": "null"
                  }
                ]
              },
              "reservations_google_maps_address": {
                "oneOf": [
                  {
                    "description": "The location of the branch on Google Maps",
                    "example": "https://maps.app.goo.gl/tpF7EMsEC7Wy5hmd6",
                    "pattern": "^https:\\/\\/maps\\.app\\.goo\\.gl\\/[A-Za-z0-9]*$",
                    "type": "string"
                  },
                  {
                    "type": "null"
                  }
                ]
              }
            }
          }
        },
        {
          "type": "null"
        }
      ]
    }
  },
  "additionalProperties": false,
  "required": [
    "name"
  ],
  "type": "object"
}
