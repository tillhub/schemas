      "minItems": 1,
      "items": {
        "type": "object",
        "additionalProperties": {
          "type": "object"
        },
        "propertyNames": {
          "enum": [
            "id",
            "owner",
            "configurations",
            "vouchers",
            "scan_prefixes",
            "voucher_actions",
            "expenses_deposits",
            "countings",
            "time_tracking",
            "settings",
            "hooks",
            "themes",
            "metadata",
            "registers",
            "branches",
            "franchise",
            "staff",
            "financials",
            "features",
            "gastro",
            "stock",
            "transactions",
            "products",
            "customers",
            "saved_cart",
            "crm",
            "datev",
            "label_printer",
            "delivery_note",
            "shore",
            "togo",
            "receipts",
            "orders",
            "carts",
            "tips",
            "emails",
            "communications",
            "taxes",
            "analytics",
            "interfaces",
            "custom_dashboards",
            "deliveries",
            "validations",
            "payments",
            "balances",
            "integrations",
            "discounts",
            "clients",
            "widgets",
            "reservations",
            "searches",
            "custom_properties",
            "ui_configurations",
            "warehub",
            "tenant",
            "onboarding",
            "reports",
            "payment_link_branding"
          ]
        },
        "required": [
          "id"
        ],
        "properties": {
          "id": {
            "type": "string",
            "format": "uuid"
          },
          "owner": {
            "type": "string",
            "format": "uuid"
          }
        }
      }
    }
  }
}
